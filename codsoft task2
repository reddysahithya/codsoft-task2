import java.util.Scanner;

public class Student_Grade_Cal {
    public static void main(String[] args) {
        Scanner Sc = new Scanner(System.in);
        System.out.printl("Grade Calculator");
        System.out.println("Enter the total number of subjects");
        int numberofsubjects = Sc.nextInt();
        int total_marks = 0;
        for (int i = 0; i < numberofsubjects; i++) {
            System.out.println("The marks obtained in " + i + ": ");
            int_Marks = Sc.nextInt();
            total_marks += _Marks;
        }
        double averagePercent = (double) total_marks / numberofsubjects;
        char Grade;
        if (averagePer >= 90) {
            Grade = 'O';
        } else if (averagePer >= 80) {
            Grade = 'A';
        } else if (averagePer >= 70) {
            Grade = 'B';
        } else if (averagePer >= 60) {
            Grade = 'C';
        } else if (averagePer >= 50) {
            Grade = 'D';
        } else if (averagePer >= 40) {
            Grade = 'E';
        } else {
            Grade = 'F';
        }
        System.out.println("Total Marks for student is " + total_marks);
        System.out.println("Average Percentage of student is " + averagePer + "%");
        System.out.println("Grade of student is " +Â Grade);
		}
	}
